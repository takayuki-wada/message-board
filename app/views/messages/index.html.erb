<div class="panel panel-default">
	<div class="panel-heading">メッセージ一覧</div>
	<div class="panel-body">
		<%# パネルの中に、既に登録済みのデータを出力 %>
		<% @messages.each do |message| %>
			<span class="label label-info"><%= message.name %></span>
			<%= message.body %>
			<br>
		<% end %>
	</div>
</div>

<% #部分テンプレートの読込み(パーシャル) %>
<%= render 'form' %>
